        <Button id="btn_view_results_leave" pos="RT|26,26,244,126" style="ImageButton"
              bk.image="#view_results_btn_leave_nor" bk_over.image="#view_results_btn_leave_nor" bk_down.image="#view_results_btn_leave_pre" text="#StrLeave"  visible="1"/>
        
        <Control pos="LT|-1,194,1436,690" style="Picture" bk.image="#view_results_panel_bg"/>
