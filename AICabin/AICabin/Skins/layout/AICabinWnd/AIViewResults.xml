          <Button id="btn_view_results_go" pos="RT|637,687,341,126" style="ImageButton"
                bk.image="#view_results_btn_go_nor" bk_over.image="#view_results_btn_go_nor" bk_down.image="#view_results_btn_go_pre" text="#StrOkGo"/>
        </Layout>

        <XML id="AILeftBtnPanelStyle"/>

        <XML id="AIRightPanelStyle"/>

        <XML id="AILeftBtnPanelUnExpendStyle"/>

