          <AutoSizeButton style="Label" pos="LT|819,222,210,48" maxwidth="400" text="#StrAccuracyRate" txt.font="font50" txt.textcolor="#N01" txt.halign="left"/>
          <AutoSizeButton id="percentage_label" style="Label" pos="RT|668,186,224,84" maxwidth="400" text="#StrPercentageZero" txt.font="font86b" txt.textcolor="#N01" txt.halign="left"/>
          <AutoSizeButton style="Label" pos="LT|819,330,210,48" maxwidth="400" text="#StrUseTime" txt.font="font50" txt.textcolor="#N01" txt.halign="left"/>
          <AutoSizeButton id="timedata_label" style="Label" pos="RT|668,330,224,48" maxwidth="400" text="#StrTimeData" txt.font="font50b" txt.textcolor="#N01" txt.halign="left"/>

